# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Block {
  number: Int!
  transactions: [Transaction!]!
}

type Transaction {
  hash: String!
  nonce: Int!
  value: String!
  gas_price: Int!
  gas: Int!
  type: Int!
  to: String!
  data: String!
}

type Query {
  blocks: [Block!]!
}

